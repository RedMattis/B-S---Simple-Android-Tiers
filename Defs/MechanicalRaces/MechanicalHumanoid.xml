<Defs>
  <HediffDef Name="BS_BaseRobotHumanoid" Abstract="True">
    <hediffClass>BigAndSmall.RaceTracker</hediffClass>

    <isBad>false</isBad>
    <everCurableByItem>false</everCurableByItem>
    <initialSeverity>1</initialSeverity>
    <modExtensions>
      <li Class="BigAndSmall.PawnExtension">
        <isMechanical>true</isMechanical>
        <banAddictionsByDefault>true</banAddictionsByDefault>
        <nullsThoughts>
          <li>Pain</li>
          <li>MissingTongue</li>
          <li>RotStink</li>
          <li>RotStinkLingering</li>
          <li>SharedBed</li>
          <li>MasochistPain</li>
          <li>ProsthophobeUnhappy</li>
        </nullsThoughts>
        <traitFilters>
          <blacklist>
            <li>BodyPurist</li>
          </blacklist>
        </traitFilters>
        <hediffFilters>
          <banlist>
            <li>Flu</li>
            <li>Malaria</li>
            <li>SleepingSickness</li>
            <li>Plague</li>
            <li>WoundInfection</li>
            <li>LungRot</li>
            <li>GutWorms</li>
            <li>MuscleParasites</li>
            <li>OrganDecay</li>
            <li>FoodPoisoning</li>

            <li>Carcinoma</li>
            <li>HeartAttack</li>
            <li>BadBack</li>
            <li>Frail</li>
            <li>Cataract</li>
            <li>Dementia</li>
            <li>Alzheimers</li>
            <li>Asthma</li>
            <li>HeartArteryBlockage</li>

            <li>Anesthetic</li>

            <li MayRequire="vanillaracesexpanded.fungoid">VRE_GeneInfection</li>
          </banlist>
        </hediffFilters>
        <geneTagFilters>
          <blacklist>
            <li>Pain</li>
          </blacklist>
        </geneTagFilters>
      </li>
    </modExtensions>
  </HediffDef>
</Defs>