      <Medicine>8</Medicine>
      <Crafting>12</Crafting>
    </skillRequirements>
    <researchPrerequisite>
      <li>BS_AndroidConversion</li>
    </researchPrerequisite>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentSpacer</li>
          </thingDefs>
        </filter>
        <count>3</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Hyperweave</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>ComponentSpacer</li>
        <li>Hyperweave</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>BS_ArtificialBrain</li>
    </appliedOnFixedBodyParts>
  </RecipeDef>

